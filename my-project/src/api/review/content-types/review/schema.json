{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Movie",
        "Video Game",
        "Book"
      ]
    },
    "Author": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "rating": {
      "min": 1,
      "max": 5,
      "type": "integer"
    },
    "Message": {
      "type": "blocks"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
